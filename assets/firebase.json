      "finalrunningorder" : {{ entry.finalrunningorder }},
      "semifinalvotes" : {{ entry.semifinalvotes }},
      "finalvotes" : {{ entry.finalvotes }},
      "semifinalvoting" : "{{ entry.semifinalvoting }}",
      "finalvoting" : "{{ entry.finalvoting }}"
